    "build": "electron-vite build && electron-builder --config .electron-builder.config.js",
    "build:dev": "yarn run typecheck && vite build",
    "typeorm": "typeorm-ts-node-commonjs -d ./cli.ts",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "yarn run build && electron-builder --win --config .electron-builder.config.js",
    "build:mac": "electron-vite build && electron-builder --mac --config .electron-builder.config.js",
    "build:linux": "electron-vite build && electron-builder --linux --config .electron-builder.config.js"
