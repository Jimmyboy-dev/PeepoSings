{
  "name": "peepo-sings",
  "version": "0.3.1",
  "description": "Desktop music player designed for Live Broadcasts - Music based on moods",
  "main": "packages/main/dist/index.cjs",
  "author": "devJimmyboy",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=v16.13",
    "npm": ">=8.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devJimmyboy/PeepoSings.git"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "build:dev": "cross-env MODE=development yarn run build",
    "precompile": "cross-env MODE=production yarn run build",
    "compile": "electron-builder build --config .electron-builder.config.js --dir --config.asar=false",
    "package": "electron-builder build --config .electron-builder.config.js -w",
    "pretest": "yarn run build",
    "test": "node tests/app.spec.js",
    "dev": "node scripts/watch.js",
    "watch": "node scripts/watch.js",
    "lint": "eslint . --ext js,ts,tsx",
    "typecheck-main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck-preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck-renderer": "tsc --noEmit -p packages/renderer/tsconfig.json",
    "typecheck": "yarn run typecheck-main && yarn run typecheck-preload && yarn run typecheck-renderer",
    "pretypecheck-renderer": "dts-cb -i packages/preload/src/**/*.ts -o packages/preload/exposedInMainWorld.d.ts"
  },
  "dependencies": {
    "@mantine/core": "3.6.9",
    "@mantine/dates": "3.6.9",
    "@mantine/hooks": "3.6.9",
    "@mantine/modals": "3.6.9",
    "@mantine/notifications": "3.6.9",
    "@reduxjs/toolkit": "^1.7.2",
    "@typescript-eslint/parser": "5.12.0",
    "auto-launch": "^5.0.5",
    "daisyui": "2.1.0",
    "dayjs": "^1.10.7",
    "dotenv": "16.0.0",
    "electron-better-ipc": "^2.0.1",
    "electron-context-menu": "^3.1.2",
    "electron-is-dev": "^2.0.0",
    "electron-serve": "^1.1.0",
    "electron-store": "^8.0.1",
    "electron-updater": "4.6.5",
    "electron-window-state": "^5.0.3",
    "fitool": "^1.0.2",
    "fluent-ffmpeg": "^2.1.2",
    "framer-motion": "^6.2.6",
    "gsap": "^3.9.1",
    "jquery": "^3.6.0",
    "lastfmapi": "^0.1.1",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "nanoid": "^3.3.1",
    "open": "^8.4.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hamburger-menu": "^1.2.1",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "readline": "^1.3.0",
    "reapop": "^4.0.3",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-persist-electron-storage": "^2.1.0",
    "sanitize-filename": "^1.6.3",
    "ytdl-core": "4.10.1",
    "ytsr": "^3.6.0"
  },
  "devDependencies": {
    "@iconify/react": "3.1.3",
    "@types/auto-launch": "^5.0.2",
    "@types/electron-devtools-installer": "2.2.1",
    "@types/express": "^4.17.13",
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/jquery": "^3.5.13",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^17.0.18",
    "@types/passport": "^1.0.7",
    "@types/react": "17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
