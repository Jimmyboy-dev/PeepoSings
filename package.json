{
  "name": "peepo-sings",
  "version": "0.2.4",
  "description": "Desktop music player designed for Live Broadcasts - Music based on moods",
  "main": "packages/main/dist/index.cjs",
  "author": "devJimmyboy",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=v16.13",
    "npm": ">=8.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devJimmyboy/PeepoSings.git"
  },
  "scripts": {
    "dev": "npm run watch",
    "build": "node scripts/build.js",
    "precompile": "cross-env MODE=production npm run build",
    "compile": "electron-builder build --config .electron-builder.config.js --dir",
    "dist": "electron-builder --config .electron-builder.config.js",
    "release": "electron-builder",
    "pretest": "npm run build",
    "test": "node tests/app.spec.js",
    "watch": "node scripts/watch.js",
    "typecheck-main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck-preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck-renderer": "tsc --noEmit -p packages/renderer/tsconfig.json",
    "typecheck": "npm run typecheck-main && npm run typecheck-preload && npm run typecheck-renderer",
    "pretypecheck-renderer": "dts-cb -i packages/preload/src/**/*.ts -o packages/preload/exposedInMainWorld.d.ts"
  },
  "dependencies": {
    "@mantine/core": "^3.6.2",
    "@mantine/dates": "^3.6.2",
    "@mantine/hooks": "^3.6.2",
    "@mantine/modals": "^3.6.2",
    "@mantine/notifications": "^3.6.2",
    "@typescript-eslint/parser": "5.10.0",
    "auto-launch": "^5.0.5",
    "daisyui": "^1.24.3",
    "dayjs": "^1.10.7",
    "dotenv": "^14.2.0",
    "electron-better-ipc": "^2.0.1",
    "electron-context-menu": "^3.1.1",
    "electron-is-dev": "^2.0.0",
    "electron-serve": "^1.1.0",
    "electron-store": "^8.0.1",
    "electron-updater": "4.6.5",
    "electron-window-state": "^5.0.3",
    "fitool": "^1.0.2",
    "fluent-ffmpeg": "^2.1.2",
    "gsap": "^3.9.1",
    "jquery": "^3.6.0",
    "lastfmapi": "^0.1.1",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "mobx-utils": "^6.0.4",
    "open": "^8.4.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hamburger-menu": "^1.2.1",
    "react-router-dom": "^6.2.1",
    "readline": "^1.3.0",
    "sanitize-filename": "^1.6.3",
    "ytdl-core": "^4.10.0",
    "ytsr": "^3.6.0"
  },
  "devDependencies": {
    "@iconify/react": "^3.1.2",
    "@types/auto-launch": "^5.0.2",
    "@types/electron-devtools-installer": "2.2.1",
    "@types/express": "^4.17.13",
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/jquery": "^3.5.13",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^17.0.10",
    "@types/passport": "^1.0.7",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@typescript/lib-dom": "npm:@types/web",
    "@vitejs/plugin-react": "^1.1.4",
    "autoprefixer": "^10.4.2",
    "cross-env": "7.0.3",
    "dts-for-context-bridge": "0.7.1",
    "electron": "^16.0.7",
    "electron-builder": "22.14.5",
    "electron-devtools-installer": "3.2.0",
    "electron-reloader": "^1.2.1",
    "eslint": "8.7.0",
    "eslint-plugin-react": "7.28.0",
    "nano-staged": "0.5.0",
    "npm-run-all": "^4.1.5",
    "playwright": "1.18.0",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-nesting": "^10.1.2",
    "simple-git-hooks": "2.7.0",
    "tailwindcss": "^3.0.15",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "tslib": "^2.3.1",
    "typescript": "4.6.0-dev.20220123",
    "vite": "^2.7.13"
  }
}
